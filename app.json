{
  "expo": {
    "name": "Impruvi",
    "slug": "impruvi-mobile-assets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/impruvi-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/impruvi-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F35156"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "10",
      "supportsTablet": true,
      "bundleIdentifier": "com.impruvi.impruvimobileassets",
      "infoPlist": {
        "NSCameraUsageDescription": "Using camera for recording drill and feedback videos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
